<Window x:Class="CommonDialogs.FilterDialog.FilterWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CommonDialogs.FilterDialog"
        mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
        <DockPanel DockPanel.Dock="Bottom" LastChildFill="False">
            <Button TabIndex="1" DockPanel.Dock="Right" Width="50" Click="OkButton_Click">OK</Button>
            <Button TabIndex="2" DockPanel.Dock="Right" Width="50" Click="CancelButton_Click">Cancel</Button>
        </DockPanel>
        <local:FilterUserControl x:Name="Filter"></local:FilterUserControl>
    </DockPanel>
</Window>
